{
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "json"
  ],
  "javascript.validate.enable": false,
  "list.source.symbols.excludes": ["**/node_modules/**/*"],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "graphql"
  ],
  "suggest.noselect": true,
  "suggest.autoTrigger": "trigger",
  "sh.enable": true
}
